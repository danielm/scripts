<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />  
  <item label="Chromium" icon="/usr/share/icons/hicolor/48x48/apps/chromium-browser.png">
    <action name="Execute">
      <command>chromium-browser</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

  <item label="EvilVTE" icon="/usr/share/icons/hicolor/48x48/apps/evilvte.png">
    <action name="Execute">
      <command>evilvte</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

  <item label="Geany" icon="/usr/share/icons/hicolor/48x48/apps/geany.png">
    <action name="Execute">
      <command>geany</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  
  <item label="Sublime Text 2" icon="/opt/sublime-text-2/Icon/48x48/sublime_text.png">
    <action name="Execute">
      <command>/opt/sublime-text-2/sublime_text</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

  <item label="Eclipse" icon="/opt/eclipse/icon.xpm">
    <action name="Execute">
      <command>/opt/eclipse/eclipse</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  
  <separator/>
  
  <item label="Pidgin" icon="/usr/share/icons/hicolor/48x48/apps/pidgin.png">
    <action name="Execute">
      <command>pidgin</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

  <separator label="System" />
  
  <item label="GTK Theme Swtich">
    <action name="Execute">
      <command>gtk-theme-switch</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
  <item label="Reconfigure Openbox">
    <action name="Reconfigure" />
  </item>
</menu>

</openbox_menu>
